{{#each folders}}

<div class="col-xs-4 col-sm-2 folder-info" data-path="{{path}}">
  <img class="icon" src="icon-folder.svg" >
  <h5>{{name}}</h5>
  <h6>Created on {{createdAt}}</h6>
  <h6>Modified on {{updatedAt}}</h6>
  <button type="button" class="btn btn-warning btn-xs">Rename</button>
  <button type="button" class="btn btn-danger btn-xs">Delete</button>
</div>

{{/each}}
